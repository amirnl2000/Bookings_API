        {
          "name": "http://{{baseURL}}:3000/login",
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "const json = pm.response.json();\r",
                  "\r",
                  "pm.test(\"Login returned token\", () => {\r",
                  "  pm.expect(json).to.have.property(\"token\");\r",
                  "  pm.expect(json.token).to.be.a(\"string\").and.not.empty;\r",
                  "});\r",
                  "\r",
                  "// The rest of the collection uses: Authorization: Bearer {{bearerToken}}\r",
                  "\r",
                  "pm.environment.set(\"bearerToken\", json.token);\r",
                  ""
                ],
                "type": "text/javascript",
                "packages": {},
                "requests": {}
              }
            }
